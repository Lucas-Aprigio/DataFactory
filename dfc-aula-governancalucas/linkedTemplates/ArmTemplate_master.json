{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfc-aula-governancalucas"
		},
		"AzureDataLakeStorageLucas_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLucas'"
		},
		"AzureMySqlSakilaLucas_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySqlSakilaLucas'"
		},
		"MySqlCarloca_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlCarloca'"
		},
		"AzureDataLakeStorageLucas_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saauladatalucas.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfc-aula-governancalucas_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLucas_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLucas_accountKey')]"
					},
					"AzureMySqlSakilaLucas_connectionString": {
						"value": "[parameters('AzureMySqlSakilaLucas_connectionString')]"
					},
					"MySqlCarloca_connectionString": {
						"value": "[parameters('MySqlCarloca_connectionString')]"
					},
					"AzureDataLakeStorageLucas_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLucas_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}